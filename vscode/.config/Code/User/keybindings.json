// Place your key bindings in this file to override the defaultsauto[]
[
  {
    "key": "ctrl+alt+u",
    "command": "cSpell.addWordToUserDictionary"
  },
  {
    "command": "vscode-neovim.send",
    "key": "ctrl+k",
    "when": "editorTextFocus && neovim.mode != insert",
    "args": "<C-k>"
  },
  {
    "command": "vscode-neovim.send",
    "key": "ctrl+l",
    "when": "editorTextFocus && neovim.mode != insert",
    "args": "<C-l>"
  },
  {
    "key": "ctrl+k ctrl+h",
    "command": "-workbench.action.output.toggleOutput",
    "when": "workbench.panel.output.active"
  },
  {
    "key": "ctrl+alt+w",
    "command": "cSpell.addWordToWorkspaceDictionary"
  },
  {
    "key": "ctrl+k ctrl+k",
    "command": "-editor.action.selectFromAnchorToCursor",
    "when": "editorTextFocus && selectionAnchorSet"
  },
  {
    "key": "ctrl+k ctrl+k",
    "command": "-editor.action.defineKeybinding",
    "when": "editorTextFocus && !editorReadonly && editorLangId == 'jsonc'"
  },
  {
    "key": "ctrl+k ctrl+0",
    "command": "editor.foldAll",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "ctrl+k ctrl+0",
    "command": "-editor.foldAll",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "ctrl+alt+f",
    "command": "references-view.findReferences",
    "when": "editorHasReferenceProvider"
  },
  {
    "key": "shift+alt+f12",
    "command": "-references-view.findReferences",
    "when": "editorHasReferenceProvider"
  },
  {
    "key": "alt+v",
    "command": "toggleVim"
  },
  {
    "key": "ctrl+shift+n",
    "command": "vscode-neovim.toggle"
  },
  {
    "key": "ctrl+alt+meta+v",
    "command": "latex-workshop.viewExternal"
  },
  {
    "key": "ctrl+shift+b",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "ctrl+b",
    "command": "-workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "ctrl+shift+e",
    "command": "-workbench.view.explorer",
    "when": "viewContainer.workbench.view.explorer.enabled"
  },
  {
    "key": "ctrl+shift+e",
    "command": "-workbench.action.quickOpenNavigatePreviousInFilePicker",
    "when": "inFilesPicker && inQuickOpen"
  },
  {
    "key": "alt+a a",
    "command": "workbench.action.focusActivityBar"
  },
  {
    "key": "alt+a z",
    "command": "workbench.action.activityBarLocation.hide"
  },
  {
    "key": "ctrl+shift+e",
    "command": "workbench.view.explorer"
  },
  {
    "key": "ctrl+k ctrl+o",
    "command": "workbench.action.quickOpen"
  },
  {
    "key": "ctrl+p",
    "command": "-workbench.action.quickOpen"
  },
  {
    "key": "ctrl+e",
    "command": "-workbench.action.quickOpen"
  },
  {
    "key": "ctrl+k o",
    "command": "-workbench.action.files.showOpenedFileInNewWindow",
    "when": "emptyWorkspaceSupport"
  },
  {
    "key": "ctrl+k ctrl+o",
    "command": "-workbench.action.files.openFolder",
    "when": "openFolderWorkspaceSupport"
  },
  {
    "key": "ctrl+k ctrl+o",
    "command": "-workbench.action.files.openLocalFolder",
    "when": "remoteFileDialogVisible"
  },
  {
    "key": "shift+alt+l",
    "command": "workbench.action.increaseViewWidth"
  },
  {
    "key": "shift+alt+k",
    "command": "workbench.action.decreaseViewWidth"
  },
  {
    "key": "alt+q",
    "command": "-seito-openfile.openFileLikeThisFile"
  },
  {
    "key": "alt+a z",
    "command": "workbench.action.toggleActivityBarVisibility"
  },
  {
    "key": "alt+a s",
    "command": "workbench.action.toggleStatusbarVisibility"
  },
  {
    "key": "ctrl+q",
    "command": "-workbench.action.quit"
  },
  {
    "key": "ctrl+k k",
    "command": "workbench.action.toggleCenteredLayout"
  },
  {
    "key": "ctrl+shift+alt+v",
    "command": "open-in-vim.open"
  },
  {
    "key": "ctrl+shift+alt+k",
    "command": "workbench.action.closeFolder",
    "when": "emptyWorkspaceSupport && workbenchState != 'empty'"
  },
  {
    "key": "ctrl+k f",
    "command": "-workbench.action.closeFolder",
    "when": "emptyWorkspaceSupport && workbenchState != 'empty'"
  }
]