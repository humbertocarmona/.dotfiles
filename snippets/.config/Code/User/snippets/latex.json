{
    "Insert figure": {
        "prefix": "ifig",
        "body": [
            "\\begin{figure}[ht]",
            "    \\centering",
            "    \\includegraphics[width = $2\\textwidth]{$1}",
            "    \\caption{$3}\\label{fig:$0}",
            "\\end{figure}"
        ],
        "description": "insert figure"
    },
    "Insert subfigure": {
        "prefix": "isubfig",
        "body": [
            "\\begin{figure}[ht]",
            "    \\centering",
            "    \\begin{subfigure}[b]{\\textwidth}",
            "         \\centering",           
            "         \\includegraphics[width = $2\\textwidth]{$1}",
            "    \\end{subfigure}",
            "    \\caption{$3}\\label{fig:$0}",
            "\\end{figure}"
        ],
        "description": "insert figure"
    },
    "Insert Citation": {
        "prefix": "icite",
        "body": "$1~\\cite{$2}$0",
        "description": "insert citation"
    },
    "Inline math": {
        "prefix": "imath",
        "body": ["\\($1\\)$0"],
        "description": "insert inline math"
    },
    "Insert equation":{
        "prefix":"ieq",
        "body":[
            "\\begin{equation}\\label{eq:$0}",
            "$1",
            "\\end{equation}"    
        ],
        "description":"Insert equation"
    },
    "Insert align":{
        "prefix":"ialign",
        "body":[
            "\\begin{align}\\label{eq:$0}",
            "$1",
            "\\end{align}"    
        ],
        "description":"Insert align"
    },
    "Reference equation": {
        "prefix": "eq.",
        "body": "$1~\\eqref{eq:$2}$0",
        "description": "insert reference to equation"
    },
    "Reference figure": {
        "prefix": "fig.",
        "body": "$1~\\ref{fig:$2}$0",
        "description": "insert reference to figure"
    },
    "BrickRed": {
        "prefix": "textred",
        "body": "\\textcolor{BrickRed}{$0}",
        "description": "brick red text"
    }
}
