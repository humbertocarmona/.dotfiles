{
    "name": "snips",
    "engines": {
        "vscode": "^1.11.0"
    },
    "contributes": {
        "snippets": [
            {
                "language": [
                    "plaintex",
                    "tex"
                ],
                "path": "./snippets/latex.json"
            },
            {
                "language": [
                    "markdown"
                ],
                "path": "./snippets/markdown.json"
            },
            {
                "language": "python",
                "path": "./snippets/python.json"
            },
            {
                "language": "julia",
                "path": "./snippets/julia.json"
            }
        ]
    }
}
