{
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "julia.symbolCacheDownload": true,
    "julia.enableTelemetry": false,
    "julia.execution.resultType": "inline, errors in REPL",
    "julia.lint.call": false,
    "cSpell.customDictionaries": {
        "custom-dictionary-user": {
            "name": "custom-dictionary-user",
            "path": "~/.cspell/en.utf-8.add",
            "addWords": true,
            "scope": "user"
        }
    },
    "git.openRepositoryInParentFolders": "always",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 500,
    "workbench.colorTheme": "GitHub Dark Default",
    "editor.fontFamily": "Hack Nerd Font",
    "editor.fontLigatures": true,
    "open-in-vim.openMethod":"kitty",
    "open-in-vim.restoreCursorAfterVim": true,
    "open-in-vim.useNeovim": true,
    "editor.minimap.enabled": false,
    "git.autofetch": true,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "git.confirmSync": false,
    "terminal.integrated.fontSize": 12,
    "editor.lineHeight": 1.5,
    "files.exclude": {
        "**/*.insyncdl": true
    },
    "[julia]": {
        "editor.defaultFormatter": "0h7z.vscode-julia-format"
    }
}