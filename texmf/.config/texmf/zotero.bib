	keywords = {Coronavirus, Epidemiology, Generation time, Illness onset, Outbreak, Statistical model, Viruses},
	file = {PDF:/home/humberto/Zotero/storage/Q4DNFLE8/2020 - International Journal of Infectious Diseases - Serial interval of novel coronavirus (COVID-19) infections.pdf:application/pdf},
}

