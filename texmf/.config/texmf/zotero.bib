	file = {PDF:/home/humberto/Zotero/storage/A8KDAMJB/1993 - Fractals - Fractal Surfaces Measurement and Applications in the Earth Sciences.pdf:application/pdf},
}

@article{Osborn2011,
	title = {Reply to Saba and Orzechowski and Schon: Methane contamination of drinking water accompanying gaswell drilling and hydraulic fracturing},
	volume = {108},
	issn = {00278424},
	doi = {10.1073/pnas.1109270108},
	abstract = {Directional drilling and hydraulic-fracturing technologies are dramatically increasing natural-gas extraction. In aquifers overlying the Marcellus and Utica shale formations of northeastern Pennsylvania and upstate New York, we document systematic evidence for methane contamination of drinking water associated with shalegas extraction. In active gas-extraction areas (one or more gas wells within 1 km), average and maximum methane concentrations in drinking-water wells increased with proximity to the nearest gas well and were 19.2 and 64 mg {CH}4 L-1 (n = 26), a potential explosion hazard; in contrast, dissolved methane samples in neighboring nonextraction sites (no gas wells within 1 km) within similar geologic formations and hydrogeologic regimes averaged only 1.1 {mgL}-1 (P {\textless} 0.05; n = 34). Average δ13C-{CH}4 values of dissolved methane in shallow groundwater were significantly less negative for active than for nonactive sites (-37 ± 7‰ and -54 ± 11‰, respectively; P {\textless} 0.0001). These δ13C-{CH}4 data, coupled with the ratios of methane-to-higher-chain hydrocarbons, and δ2H- {CH}4 values, are consistent with deeper thermogenic methane sources such as the Marcellus and Utica shales at the active sites and matched gas geochemistry from gas wells nearby. In contrast, lower-concentration samples from shallow groundwater at nonactive sites had isotopic signatures reflecting a more biogenic or mixed biogenic/ thermogenic methane source. We found no evidence for contamination of drinking-water samples with deep saline brines or fracturing fluids. We conclude that greater stewardship, data, and - possibly - regulation are needed to ensure the sustainable future of shale-gas extraction and to improve public confidence in its use.},
	pages = {8172--8176},
	number = {20},
	journaltitle = {Proceedings of the National Academy of Sciences of the United States of America},
	author = {Osborn, Stephen G. and Vengosh, Avner and Warner, Nathaniel R. and Jackson, Robert B.},
	date = {2011},
	pmid = {21555547},
	keywords = {Groundwater, Formation waters, Isotopes, Organic-rich shale, Water chemistry},
	file = {PDF:/home/humberto/Zotero/storage/UVAG72HH/2011 - Proceedings of the National Academy of Sciences of the United States of America - Reply to Saba and Orzechowski and Schon Methane.pdf:application/pdf},
}

