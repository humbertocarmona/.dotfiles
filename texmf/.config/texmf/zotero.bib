	keywords = {Breakdown pressure, Fracture permeability, Hydraulic {fracturingSiltstone}, Shale gas unconventional oil and gas},
	file = {PDF:/home/humberto/Zotero/storage/N2ITBD77/applsci-07-00664.pdf:application/pdf},
}

@article{ebrahimi_multi-scale_2014,
	title = {Multi-scale approach to invasion percolation of rock fracture networks},
