	note = {Publisher: American Association for the Advancement of Science},
	file = {PDF:/home/humberto/Zotero/storage/5GHC9T3F/2020 - Science - The effect of human mobility and control measures on the COVID-19 epidemic in China(2).pdf:application/pdf},
