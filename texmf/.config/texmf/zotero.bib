	author = {Rodrigues-Filho, {JL}. and Abe, {DS}. and Gatti-Junior, P. and Medeiros, {GR}. and Degani, {RM}. and Blanco, {FP}. and Faria, {CRL}. and Campanelli, L. and Soares, {FS}. and Sidagis-Galli, {CV}. and Teixeira-Silva, V. and Tundisi, {JEM}. and Matsmura-Tundisi, T. and Tundisi, {JG}.},
	urldate = {2021-11-16},
	date = {2015-08},
	note = {Publisher: Instituto Internacional de Ecologia},
	keywords = {Amazonian rivers, hydropower dam, limnology, water quality, watershed},
	file = {PDF:/home/humberto/Zotero/storage/8XUKSDNK/full-text.pdf:application/pdf},
}

