@article{Yang2008,
	title = {Self-affine fractals embedded in spectra of complex networks},
	volume = {77},
	issn = {15393755},
	doi = {10.1103/PhysRevE.77.045101},
	abstract = {The scaling properties of spectra of real world complex networks are studied by using the wavelet transform. It is found that the spectra of networks are multifractal. According to the values of the long-range correlation exponent, the Hust exponent H, the networks can be classified into three types, namely, H{\textgreater}0.5, H=0.5, and H{\textless}0.5. All real world networks considered belong to the class of H 0.5, which may be explained by the hierarchical properties. © 2008 The American Physical Society.},
